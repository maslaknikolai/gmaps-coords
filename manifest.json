{
    "name": "GMAPS Coords",
    "version": "1.1.0",
    "description": "Helps to search coordinates in Google Maps",
    "author": "Nikolai Maslak",
    "manifest_version": 3,
    "action": {
      "default_title": "GMAPS Coords"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.google.com/maps/*"],
        "js": ["dist/gmaps-coords.user.js"]
      }
    ]
}